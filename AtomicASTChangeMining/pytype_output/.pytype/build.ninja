rule infer
  command = /Library/Frameworks/Python.framework/Versions/3.7/bin/python3.7 -m pytype.single --imports_info $imports --module-name $module -V 3.7 -o $out --no-report-errors --nofail --quick $in
  description = infer $module
rule check
  command = /Library/Frameworks/Python.framework/Versions/3.7/bin/python3.7 -m pytype.single --imports_info $imports --module-name $module -V 3.7 -o $out --analyze-annotated --no-report-errors --nofail --protocols --quick $in
  description = check $module
